{
  "rewrites": [
    {
      "source": "/register",
      "destination": "/public/index.html"
    },
    {
      "source": "/login",
      "destination": "/public/index.html"
    },
    {
      "source": "/chat",
      "destination": "/public/index.html"
    },
    {
      "source": "/user/me",
      "destination": "/public/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "server.cjs"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}